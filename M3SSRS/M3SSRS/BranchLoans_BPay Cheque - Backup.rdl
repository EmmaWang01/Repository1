      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>with c as 
(
select
	RTM_IDLink_RMR
	,rtm_idlink_xtrm
	,RTM_DateC
	,RTM_ValueCR

from iO_Product_Transaction with(nolock)
where RTM_IDLink_XTRM in
(
	'{857a90f4-efa8-4c9f-abbf-19b7057a7b79}'
	,'{8b80a451-3d1a-4097-81ea-959dd2f0d8f6}'
	,'{a8136ec2-6d41-4df2-97e2-a92baa39f139}'
	,'{2f97e453-ddcd-49b7-958d-e85460e5c3cf}'
	,'{f03763e2-eb24-4ca9-bef2-e525ade90ce8}'
	,'{274ab776-dbd9-4187-9621-2a5f08c68ab4}'
	,'{f6260e2e-aa78-400a-8c8d-17af33ae0aaf}'
) and RTM_DateC&gt;dateadd(year,-2,getdate())
)
select
	RMR.RMR_SeqNumber as 'LoanID'
	,Trans.RTM_ValueCR as 'Payment'
	,Trans.RTM_DateC as 'ProcessDate'
	,xtrm.XTRM_Detail as 'Payment Type'
	,cmr.CMR_Name as 'Branch'
from c Trans
inner join iO_Product_MasterReference RMR with(nolock) on RMR.RMR_ID=Trans.RTM_IDLink_RMR
left Join iO_Control_TransactionMaster xtrm on Trans.rtm_idlink_xtrm=xtrm.xtrm_id
left join iO_Link_MasterReference LMR with(nolock) on LMR.LMR_IDLink_Code_ID=RMR.RMR_ID and LMR.LMR_IDLink_Association='{b55145aa-2697-43b5-9c6a-c4a0960823d8}'
left join iO_Client_MasterReference cmr with(nolock) on LMR.LMR_IDLink_CMR=cmr.CMR_ID
where cmr.CMR_Name not in ('Collections Branch','Loan Centre','Personal Loans','Micro Motor')</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>

        <QueryParameters>
          <QueryParameter Name="@dateRange">
            <Value>=Parameters!fromDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SET DATEFORMAT dmy
--Declare @dateRange int;
Declare @dateFrom date;
Declare @dateTo Date;
Set @dateTo=GETDATE();

--Set @dateRange=1
--1,This month; 2,2 months; 3,This calendar year; 4,all

set @dateFrom=case @dateRange 
	when 2 then dateadd(MONTH,-2,(convert(char(6),getdate(),112)+'01'))
	when 3 then convert(DATE,'01/01/' + cast((DATEPART(year, getdate())) AS CHAR(04)))
	when 1 then convert(DATE, '01/' + cast((DATEPART(month, getdate())) AS CHAR(02)) + '/' + cast((DATEPART(year, getdate())) AS CHAR(04)))
	when 4 then '19000101'
end;


SELECT 
		SalesReport.DSS_LoanNumber,
		SalesReport.DSS_ClientName,
		SalesReport.DSS_SettlementDate,
		SalesReport.DSS_BrokerName,
		SalesReport.DSS_Status,
		CMAssess.CMR_Name as brokercontact,
		isnull(APP.RCB_CurrentValue, 0) AS 'Loan Amount',
		/*SalesReport.DSS_ApplicationFees,
		SalesReport.DSS_TotalInterest,
		SalesReport.DSS_MonthlyServiceFee,*/
		(SalesReport.DSS_ApplicationFees) + (SalesReport.DSS_TotalInterest) + (SalesReport.DSS_MonthlyServiceFee) AS 'Fees',
		/*SalesReport.DSS_CashOut,
		SalesReport.DSS_Insurance,
		SalesReport.DSS_Brokerage,*/
		(SalesReport.DSS_CashOut) + (SalesReport.DSS_Insurance) + (SalesReport.DSS_Brokerage)AS 'NAF'
		,CMobil.CCD_Details
		,Branch.CMR_Name
		
FROM M3_MAIN.dbo.iO_DataStorage_SalesReport SalesReport WITH (NOLOCK)
--Loan
LEFT JOIN iO_Product_MasterReference RMR WITH (NOLOCK)
	ON SalesReport.DSS_LoanNumber = RMR.RMR_SeqNumber
--Link Connection
LEFT JOIN iO_Link_MasterReference LMR WITH (NOLOCK)
	ON RMR.RMR_ID = LMR.LMR_IDLink_Code_ID

--Link Branch
left join iO_Link_MasterReference BranchLMR WITH (NOLOCK) on RMR.RMR_ID=BranchLMR.LMR_IDLink_Code_ID and BranchLMR.LMR_IDLink_Association = '{7e504c4d-821c-4623-a928-28ee65c3b8c8}'
left join iO_Client_MasterReference as Branch with(nolock) on Branch.CMR_ID=BranchLMR.LMR_IDLink_CMR 
		
		--'{b55145aa-2697-43b5-9c6a-c4a0960823d8}'
--Client Link
LEFT JOIN iO_Client_MasterReference CM WITH (NOLOCK)
	ON CM.CMR_SeqNumber = SalesReport.DSS_ClientNumber

--Loan Assignment Link
LEFT JOIN iO_Control_LinkMaster CLM WITH (NOLOCK)
	ON CLM.XLK_ID = LMR.LMR_IDLink_Association
--Broker Contact
LEFT JOIN iO_Client_MasterReference CMAssess WITH (NOLOCK)
	ON CMAssess.CMR_ID = LMR.LMR_IDLink_CMR

--Loan Amount
LEFT JOIN iO_Product_ControlBalance APP ON APP.RCB_IDLink_RMR = LMR.LMR_IDLink_Code_ID
              AND APP.RCB_IDLink_XRBl = '{f6e26460-bf66-40fd-9bb2-112ebc2f2b07}'

--Client Contact detail, get mobile from max sequence number to remove duplicates, time consuming
left Join (select CCD_IDLink_CMR, max(CCD_SeqNumber) as CCD_Seq from iO_Client_ContactDetail With(nolock) where CCD_IDLink_XCT='{29411831-e939-4357-940a-44f55b4a5c9b}' group by CCD_IDLink_CMR)  as CCD
	On CM.CMR_ID=CCD.CCD_IDLink_CMR
left join iO_Client_ContactDetail CMobil with(nolock) on CCD.CCD_Seq=CMobil.CCD_SeqNumber 

WHERE XLK_Detail = 'Loan\Broker  Contact'
	--AND Branch.Branch LIKE 'Tannaster%'
	--date Conversion
	--AND SalesReport.DSS_SettlementDate &gt;= dateadd(MONTH,-2,(convert(char(6),getdate(),112)+'01'))
			--show only following Brokers
	AND SalesReport.DSS_SettlementDate BETWEEN @dateFrom
		AND GETDATE()
	AND CMAssess.CMR_ID IN (
		--'{fc0c4547-30c2-4f0a-92e7-70940009e9e3}' --HOTCHIN Nick
		 '{d4e437ca-b05e-4d24-bd54-ab24a8331b1d}' --//CABLE Lauren
		,'{ce73556b-6735-42c8-9b44-0a139face678}' --//LUU Jimmy
		,'{938dd753-f827-498c-9341-c4bffd406f01}' --//GODFREY Jess
		--,'{3f953ee4-a2e3-47a0-86e4-5cb7afe3499a}' --//CLARKE Marshall
		--,'{6c506f08-7ef8-4410-b8a7-c7a14f320554}' --//KOLAK Kelly
		,'{41466703-e271-4634-abd0-b7dcd7d8e523}' --//CAMPBELL Morgan
		--,'{e2a2c8b2-4fb9-4f71-98ca-e7fa99a52d6d}' --//PUCCIO Kerri
		,'{d78407f4-c9d5-4917-b4db-3d9ebec4dd02}' --//BYRNES Patricia
		--,'{c313c51a-be1c-4cd2-a857-29afa814d33d}' --//ELLIOTT Elise 
		--,'{8a1a2380-43eb-4a57-8aa3-3ef670e5ba6d}' --//KHAIKILEVITCH Alex
		,'{bd8d6bbf-c86a-486f-aa1e-12b7a4a6934d}' --//COOTE Maria
		,'{6fd9b15f-bb08-40eb-9853-a823747a42f5}' --//DURAY Luke 
		,'{608d8a9d-b58d-4507-ada6-e5aff91fbdb7}' --//CENTOFANTI Carmel 
		,'{532d11fe-4119-4b3b-8b18-bf5bcde45201}' --//Saiid Adam
		,'{0c6edd5f-36d6-4895-b4dd-ef100fd0589f}' --//Brodie Odermatt
		)
	--and SalesReport.DSS_LoanNumber='1748642'
ORDER BY DSS_SettlementDate--DSS_LoanNumber</CommandText>
      </Query>

      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>--Select top 5 * from iO_Client_BankDetail where  CBD_Default=0
-- select top 5 * from iO_Control_SystemBankBSB

If Object_ID('temp..#PaymentSchedule') is not null
Drop Table #PaymentSchedule

Select 
	RPSD_IDLink_RMR
	,min(RPSD_PaymentDueDate) As 'PaymentDueDate'
into #PaymentSchedule 
From iO_Product_PaymentScheduleDetail With(nolock)
Where RPSD_PaymentDueDate&gt;GETDATE() 
Group By RPSD_IDLink_RMR

Select distinct
	RMR.RMR_SeqNumber As 'LonaNo'
	,CMRClient.CMR_SeqNumber As 'ClientNo'
	,CMRClient.CMR_Name As 'ClientName'
	,CMRBranch.CMR_Name As 'Branch'
	,RPSD.PaymentDueDate As 'Next PaymentDueDate'
	,CBD.CBD_AccountBSBNo As 'BSB'
	,CBD.CBD_AccountNumber As 'AccountNumber'
	,CBD.CBD_AccountName As 'AccountName'
	,CBD.CBD_Default As 'isDefault'
	,SysBSB.XSYSbsb_BankName As 'BankName' 
From #PaymentSchedule RPSD
Inner Join io_product_MasterReference RMR  With(nolock) on RMR.RMR_ID=RPSD.RPSD_IDLink_RMR
Inner Join io_Link_MasterReference LMRClient  With(nolock) on RMR.RMR_ID=LMRClient.LMR_IDLink_Code_ID and LMRClient.LMR_IDLink_Association='{146afcaa-059b-469e-a000-0103e84144dc}' --Loan\Principal Borrower
Inner Join iO_Client_MasterReference CMRClient  With(nolock) on LMRClient.LMR_IDLink_CMR=CMRClient.CMR_ID
Left Join io_link_masterreference LMRBranch With(nolock) on RMR.RMR_ID=LMRBranch.LMR_IDLink_Code_ID and LMRBranch.LMR_IDLink_Association='{b55145aa-2697-43b5-9c6a-c4a0960823d8}'  --Loan\Branch
Left Join io_Client_MasterReference CMRBranch With(nolock) on LMRBranch.LMR_IDLink_CMR=CMRBranch.CMR_ID
Left Join iO_Client_BankDetail CBD With(nolock) on CBD.CBD_IDLink_CMR=CMRClient.CMR_ID and CBD.CBD_Default=1
left join iO_Control_SystemBankBSB SysBSB on CBD.CBD_AccountBSBNo = XSYSbsb_BSBNumber
where 
	(
		CBD_AccountBSBNo is null 
		or CBD_AccountNumber is null 
		or CBD_AccountName is null 
		or len(CBD_AccountBSBNo)&lt;6
		or len(CBD_AccountNumber)&gt;15
		or len(CBD_AccountNumber)&lt;4
		or SysBSB.XSYSbsb_BankName is null
	) and CBD.CBD_Default=1

order by CMRBranch.CMR_Name, CMRClient.CMR_SeqNumber</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
